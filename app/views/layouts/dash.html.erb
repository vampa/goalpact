<!DOCTYPE <!doctype html>
<html lang="en" id="dash">
<%= render "head" %>
<body>
    <section id="header-wrap">
        <a href="" class="button accent header-new-goal">Set New Goal</a>
        <div class="header-goal">
            <span class="goal-name">Chicago Marathon</span>
            <ul class="goal-name-dropdown" style="display: none;">
                <li>View Goal Profile</li>
                <li>Share Goal</li>
            </ul>
        </div>
        <a href="<%= root_path %>" id="logo">
            <img src="<%= asset_path('icon.png') %>" />
        </a>
        <%= link_to("Logout", destroy_user_session_path, :method => "delete", :class => "logout-link") %>
    </section>

    <div id="center-wrap">
        <%= yield %>
    </div>
</body>
</html>